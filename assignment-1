QUS N.15

#include <stdio.h>

int main()
{
    int l,b,h;
    int v;
    
    printf("enter tha value of l=:");
    scanf("%d",&l);
    printf("enter tha value of b=:");
    scanf("%d",&b);
    printf("enter tha value of h=:");
    scanf("%d",&h);
    v=l*b*h;
    printf("vloume of cube=%d",v);
    return 0;
}




