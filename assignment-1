QUS N.13

// AREA OF RECTANGLE

#include <stdio.h>

int main()
{
    int lenghth,width;
    int area;
    printf("enter tha lenghth a rectangle:");
    scanf("%d",&lenghth);
    printf("enter tha width a rectangle:");
    scanf("%d",&width);
    area=lenghth*width;
    printf("area of a rectangle:%d",area);
    return 0;
}


