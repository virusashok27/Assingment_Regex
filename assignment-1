QUS N.12

//AREA OF TRINGLE

#include <stdio.h>

int main()
{
    int base,height;
    float area;
    printf("enter a base of triangle:\n");
    scanf("%d",&base);
    printf("enter a height of triangle:");
    scanf("%d",&height);
    area=(base*height)/2;
    printf("entar area of triangle:%f",area);
    return 0;
}

