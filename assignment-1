QUS N.1

#include <stdio.h>

int main()
{
    int a=23;
    int b=123456;
    float c=3.456;
    printf("value of a:%d\n",a);
    printf("value of b:%d\n",b);
    printf("value of c:%f",c);
    
    return 0;
}

